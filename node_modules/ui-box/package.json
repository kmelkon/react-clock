{
  "name": "ui-box",
  "version": "1.4.0",
  "description": "Blazing Fast React UI Primitive",
  "contributors": [
    "Jeroen Ransijn (https://twitter.com/jeroen_ransijn)",
    "Roland Warmerdam (https://roland.codes)"
  ],
  "keywords": [
    "react"
  ],
  "repository": "segmentio/ui-box",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "xo && nyc ava",
    "prepublishOnly": "yarn run build",
    "precommit": "lint-staged",
    "dev": "start-storybook -p 9009",
    "build": "babel --out-dir lib --ignore *.test.js src",
    "build-storybook": "build-storybook -s .storybook/static -o .out",
    "release": "np --no-publish",
    "benchmark": "echo ui-box && react-benchmark tools/benchmarks/box.js && echo glamor/jsxstyle && react-benchmark tools/benchmarks/glamor-jsxstyle.js",
    "size": "size-limit",
    "coverage": "nyc report --reporter=html"
  },
  "dependencies": {
    "@emotion/hash": "^0.6.5",
    "glamor": "^2.20.0",
    "inline-style-prefixer": "^4.0.2",
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.10",
    "@storybook/storybook-deployer": "^2.2.0",
    "ava": "^0.25.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "nyc": "^12.0.2",
    "react": "^16.4.2",
    "react-benchmark": "^1.1.0",
    "react-dom": "^16.4.2",
    "react-test-renderer": "^16.4.2",
    "sinon": "^6.1.5",
    "size-limit": "^0.19.2",
    "xo": "^0.22.0"
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": "xo-react",
    "envs": [
      "node",
      "browser"
    ],
    "prettier": true,
    "space": true,
    "semicolon": false
  },
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "git add"
    ]
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "19 KB"
    }
  ],
  "ava": {
    "require": [
      "babel-register",
      "./tools/enzyme-config"
    ],
    "babel": "inherit"
  }
}
